module random_gen_row (
    input clk,  // clock
    input rst,  // reset
    input trigger,
    output out[16]
  ) {
  pn_gen pn_gen1(.clk(clk),.rst(rst),.seed(32h100)); 
  
  always {
 
    pn_gen1.next = trigger;
    if (pn_gen1.num[1:0] != b00 & pn_gen1.num[3:2] != b00){
      out[3:0] = pn_gen1.num[3:0];
    }
     else{
    out = 16b0101;
      }
 
    }
}
