module compare (
    input z,
    input v,
    input n,
    input alufn_signal[4],
    output out[16] 
  ) {
  
  always {
    out = 16b0;
    case (alufn_signal) {
      default: out[0] = 0;
      b0011: out[0] = z;
      b0100: out[0] = n ^ v;
      b0101: out[0] = z | (n ^ v);
    }
  }
}