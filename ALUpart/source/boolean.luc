module boolean (
    input a[16],
    input b[16],
    input alufn_signal[5],
    output out[16]
  ) {

  
  always {

    case(alufn_signal[3:0]){
    
      b0110: out = a & b;
      b0111: out = a | b;
      b1000: out = a ^ b;
      b1001: out = a;
      default:
        out = b0000000000000000;
    }
   
  }
}