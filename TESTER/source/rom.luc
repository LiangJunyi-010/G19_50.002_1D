module rom (
    input address[5],
    output oA[16],
    output oB[16],
    output alufn[6],
    output oZVN[3],
    output out[16]
  ) {
  const A = {16b11110};
  const B = {16b11111};
  const ALUSGNL = {5b0};
  const ZVN = {};
  const O = {16b111101};

  always {
    out = O[address];
    oA = A[address];
    oB = B[address];
    alufn = ALUSGNL[address];
    oZVN = ZVN[address];
  }
}
